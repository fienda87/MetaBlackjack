{
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": true,
      "decorators": false,
      "dynamicImport": true
    },
    "transform": {
      "react": {
        "runtime": "automatic",
        "development": false,
        "refresh": true
      }
    },
    "minify": {
      "compress": {
        "unused": true,
        "drop_console": true,
        "drop_debugger": true
      },
      "mangle": true
    }
  },
  "minify": true,
  "module": {
    "type": "esnext"
  },
  "env": {
    "production": {
      "jsc": {
        "minify": {
          "compress": true,
          "mangle": true
        }
      }
    }
  }
}
